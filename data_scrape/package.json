    "cheerio": "^1.0.0-rc.12",
    "i": "^0.3.7",
    "npm": "^10.3.0",
    "playwright": "^1.41.0",
    "puppeteer": "^21.7.0",
    "random-useragent": "^0.5.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^14.0.0",
    "@types/pg": "^8.10.9",
    "@types/random-useragent": "^0.3.3",
    "express": "^4.18.2",
    "nodemon": "^3.0.3",
    "pg": "^8.11.3",
    "ts-node": "^10.9.2",
    "tsc-watch": "^4.0.0",
    "typescript": "^4.0.0"
  }
}
